#include <stdio.h>
#include <string.h>

int main()
{
    int t;
    scanf("%d", &t);

    for (int i = 1; i <= t; i++)
    {
        int n;
        scanf("%d", &n);

        char a[n];
        scanf("%s", a);

        int m;
        scanf("%d", &m);
        char b[m];
        char c[m];

        scanf("%s", b);
        scanf("%s", c);

        char res[200];
        strcpy(res, a);

        for (int i = 0; i < m; i++)
        {
            char st1[100];

            if (c[i] == 'V')
            {
                char temp[200];
                temp[0] = b[i];
                temp[1] = '\0';
                strcat(temp, res);
                strcpy(res, temp);
            }

            else
            {
                int len = strlen(res);
                res[len] = b[i];
                res[len + 1] = '\0';
            }
        }
        printf("%s\n", res);
    }

    return 0;
}
